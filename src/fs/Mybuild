package embox.fs

module node {
    @IncludePath("$(SRC_DIR)/compat/linux/include")
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "node.c"
}

module file_desc {
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "file_desc.c"
}

module rootfs {
	source "rootfs.c"
	depends node, driver.repo
}

module core {
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "file.c",
		"vfs.c",
		"fs.c"
	depends file_desc, node, rootfs
	depends embox.fs.driver.repo
	depends embox.kernel.task_api
}
