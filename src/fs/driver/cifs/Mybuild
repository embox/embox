package embox.fs.driver

module cifs {
	depends third_party.samba.core

	option number cifs_descriptor_quantity=4

	source "order.mk"

	@IncludePath("$(THIRDPARTY_DIR)/samba/include")
	@IncludePath("$(ROOT_DIR)/build/samba/include")
	source "cifs.c"
}

@Build(stage=2,script="true")
@BuildDepends(third_party.samba_new.core)
module cifs_new {
	depends third_party.samba_new.core

	option number cifs_descriptor_quantity=4

	@IncludePath("$(THIRDPARTY_DIR)/samba_new/include")
	source "cifs.c"
}
