	option string log_level="LOG_ERR"

	source "alphasort.c"
	source "chdir.c"
	source "chmod.c"
	source "chown.c"
	source "creat.c"
	source "fsync.c"
	source "getcwd.c"
	source "lseek.c"
	source "openat.c"
	source "pread.c"
	source "pwrite.c"
	source "realpath.c"
	source "rewinddir.c"
	source "scandir.c"
	source "stat.c"
	source "truncate.c"
	source "umask.c"

	@NoRuntime depends embox.kernel.task.idesc
	@NoRuntime depends embox.kernel.task.resource.errno
