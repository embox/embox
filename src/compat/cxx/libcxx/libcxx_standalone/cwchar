	using ::vswprintf;
	using ::wcstol;
	using ::wcstoul;
	using ::wcstoll;
	using ::wcstoull;
	using ::wcstof;
	using ::wcstod;
	using ::wcstold;
}

#endif // LIBCXX_STANDALONE_CWCHAR_
