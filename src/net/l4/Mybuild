package embox.net

module bootp {
	source "bootp.c"
	depends udp
}

module dns {
	source "dns.c"
	option string nameserver="10.0.0.11"

	depends embox.compat.posix.net.socket
	depends embox.lib.LibC
	depends embox.mem.heap_api
}

module ntp {
	source "ntp.c"

	depends udp
	depends embox.kernel.time.timekeeper
	depends embox.kernel.timer.sys_timer
}

module pop3 {
	source "pop3.c"

	depends embox.compat.posix.net.gethostbyname
	depends embox.compat.posix.net.socket
	depends embox.lib.LibC
}

module smtp {
	source "smtp.c"

	depends embox.lib.LibCMisc
	depends embox.compat.posix.net.gethostbyname
	depends embox.compat.posix.net.socket
	depends embox.lib.LibC
}

module snmp {
	source "snmp.c"

	depends embox.net.mib.snmp_mib
}
