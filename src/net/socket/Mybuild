package embox.net

module socket {
	source "socket.c", "socket_allocator.c", "socket_repo.c"
	source "kernel_socket.c", "port.c"
	make flags "-I$(SRC_DIR)/compat/posix/include"
	make flags "-I$(SRC_DIR)/compat/linux/include"
}

module af_packet {
	source "af_packet.c"
	depends socket

}
module af_unix {
	source "af_unix.c"
	depends socket
}
