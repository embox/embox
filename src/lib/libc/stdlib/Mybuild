package embox.lib

static module LibCRand {
	source "rand.c"
	source "random.c"
	source "random_r.c"
}


static module LibCStdlib {
	source "bsearch.c"
	source "qsort.c"
	source "strtod.c"
	source "strtol.c"
	source "strtoul.c"
	source "abort.c"
	source "system.c"
	source "strtoll.c"
	source "strtoull.c"
	source "atol.c"

	@NoRuntime depends LibCRand /* qsort required*/
}
