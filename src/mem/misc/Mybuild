package embox.mem

interface iobjalloc {
	feature alloc
	feature rt_alloc extends alloc
}

module slab {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "slab.c", "slab_impl.h"
	provides iobjalloc.alloc
	depends embox.mem.page_api, embox.mem.heap_lds
}

module pool {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "pool.c"
	provides iobjalloc.rt_alloc
}
