
package embox.pnet

abstract module entry_api { }

module socket {
	source "socket.c"
}

module pnet_entry {
    @IncludePath("$(SRC_DIR)/compat/linux/include")
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "pnet_entry.c"
	depends embox.pnet.core
}

module net_entry extends entry_api {
    @IncludePath("$(SRC_DIR)/compat/linux/include")
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "net_entry.c"
	depends embox.pnet.core, embox.pnet.netfilter.matcher, pnet_entry
}
