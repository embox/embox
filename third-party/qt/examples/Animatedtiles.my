@BuildDepends(third_party.qt.core)
@Cmd(name = "animatedtiles",
	help = "animation qt's example",
	man = '''
		AUTHORS
			Ilia Vaprol
	''')
module animatedtiles {
	source "^BUILD/extbld/^MOD_PATH/animatedtiles.embox.o"

	@NoRuntime depends third_party.qt.core
}
