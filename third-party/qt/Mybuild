package third_party.qt

@Build(script="$(MAKE) -C $(THIRDPARTY_DIR)/qt MAKEFLAGS= EMBOX_MAKEFLAGS='$(MAKEFLAGS)' ROOT_DIR=$(abspath $(ROOT_DIR)) EMBOX_CFLAGS='$(CFLAGS)' EMBOX_CXXFLAGS='$(CXXFLAGS)' EMBOX_CPPFLAGS='$(EMBOX_EXPORT_CPPFLAGS)'")
module core {
	depends embox.lib.LibC
	depends embox.lib.LibCXX
	depends embox.arch.x86.LibM // FIXME:
	// Posix
}

module example extends embox.lib.CXXApp {
	depends core
	depends embox.lib.LibCXX
}
