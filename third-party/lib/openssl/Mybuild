package third_party.lib

@BuildArtifactPath(cppflags="-I$(EXTERNAL_BUILD_DIR)/third_party/lib/openssl/install/include")
@Build(stage=1,script="$(EXTERNAL_MAKE)")
module openssl {	
	option string api=""

	@AddPrefix("^BUILD/extbld/^MOD_PATH/install/lib")
	source "libssl.a", "libcrypto.a"

	@NoRuntime depends embox.compat.posix.time.nanosleep
	@NoRuntime depends embox.compat.atomic.pseudo_atomic
}
