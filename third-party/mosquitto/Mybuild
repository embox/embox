package third_party.cmd

@App
@AutoCmd
@Build(stage=2,script="$(EXTERNAL_MAKE)")
@Cmd(name = "mosquitto",
	help = "MQTT Broker",
	man = '''
		NAME
			mosquitto - MQTT Broker
		SYNOPSIS
			nano [OPTIONS] [[+LINE,COLUMN] FILE]...
		AUTHORS
			Yuriy Paramonov - Adaptation for Embox
	''')
module mosquitto {
	source "^BUILD/extbld/^MOD_PATH/install/mosquitto.o"
}
