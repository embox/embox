package third_party.bsp.stmf3cube

@BuildArtifactPath(cppflags="-DUSE_RTOS=0 -DUSE_HAL_DRIVER $(addprefix -I$(EXTERNAL_BUILD_DIR)/third_party/bsp/stmf3cube/cube/STM32CubeF3-1.11.0/Drivers/,STM32F3xx_HAL_Driver/Inc CMSIS/Device/ST/STM32F3xx/Include CMSIS/Include)")
module cube_cppflags {

}

@Build(stage=1,script="$(EXTERNAL_MAKE) download extract patch")
@BuildDepends(cube_cppflags)
@BuildDepends(third_party.bsp.stmf3cube.stm32f3_conf)
static module cube {

	@Cflags("-Wno-unused")
	@Cflags("-Wno-undef")
